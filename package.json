{
  "name": "test-app",
  "type": "module",
  "private": true,
  "dependencies": {
    "@pikku/core": "^0.9.7",
    "@pikku/jose": "^0.9.3",
    "@pikku/schema-cfworker": "^0.9.2",
    "eventsource": "^4.0.0",
    "@pikku/ws": "^0.9.2",
    "tslib": "^2.7.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@pikku/cli": "^0.9.7",
    "@pikku/fetch": "^0.9.2",
    "@pikku/websocket": "^0.9.2",
    "@types/node": "^22",
    "concurrently": "^9.2.0",
    "typescript": "^5.6",
    "@types/ws": "^8"
  },
  "scripts": {
    "pikku": "pikku all",
    "test:template": "bash run-tests.sh --websocket",
    "test:http": "npx tsx client/http-fetch.ts",
    "test:sse": "npx tsx client/http-sse.ts",
    "test:rpc": "npx tsx client/rpc.ts",
    "test:websocket": "npx tsx client/websocket.ts",
    "start": "tsx src/start.ts",
    "dev": "tsx --watch src/start.ts",
    "postinstall": "pikku all",
    "test": "npm run test:http && npm run test:websocket",
    "stackblitz": "concurrently 'pikku --watch --silent' 'npm run dev'"
  },
  "main": "src/start.ts",
  "stackblitz": {
    "startCommand": "npm run stackblitz"
  }
}